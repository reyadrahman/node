    "isFake": true,
    "prefs": {},
    "publisherId": "us-east-1:f539ddf1-ba70-46b4-a37e-d5988c92494c",
    "userRole": "user"
}

await bridge.saveUser(
    session.getIdToken().getJwtToken(),
    'e3a6ba00-805c-11e6-94cd-f9ef165f7fb2',
    'messenger',
    'dummy::7afd46e4f3b74575',
    null,
    'admin',
);

const result =
{
    "botId_channel_email": "e3a6ba00-805c-11e6-94cd-f9ef165f7fb2__messenger__sean@plumcake.engineer",
