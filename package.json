  "scripts": {
    "start": "npm run build && npm run run-server",
    "run-server": "node ./dist-server/bundle.js",
    "clean": "npm run clean-client && npm run clean-server",
    "clean-client": "rm -rf dist-client",
    "clean-server": "rm -rf dist-server",
    "build": "bash ./scripts/build.sh",
    "build-client": "npm run clean-client && webpack --config webpack-config-client.js --color --progress",
    "build-server": "npm run clean-server && webpack --config webpack-config-server.js --color --progress",
    "test": "mocha-webpack --colors --webpack-config webpack-config-test.js \"src/server/tests/**/*.test.js\""
